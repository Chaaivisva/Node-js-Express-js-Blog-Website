<div class="author">
  <div class="author_heading">Search Results</div>
</div>

<section class="articles">
  <ul class="article-ul">
    <% data.forEach(post => { %>
      <li>
        <a href="/post/<%= post._id %>">
          <span><%= post.title %></span>
          <span class="author-list_date"><%= post.createdAt.toDateString() %></span>
        </a>
      </li>
     <% }) %>
  </ul>
</section>